      JOIN YS_DB.YSH_SC_DEVICE d ON z.ASSETS_NAME=d.col3 AND d.col0 LIKE '%.'||z.STATION_LINE
      WHERE z.ASSETS_CATEGORY LIKE '变电%' and z.ID IS NULL
      ]]>
